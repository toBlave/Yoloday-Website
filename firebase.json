{
  "hosting": [
    {
      "target": "yoloday-public",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "redirects": [
        {
          "source": "**",
          "destination": "https://yolo.day/**",
          "type": 301
        }
      ]
    }
  ]
}
